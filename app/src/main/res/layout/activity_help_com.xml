<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Help_com">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="130dp"
            android:layout_height="46dp"
            android:layout_marginTop="28dp"
            android:paddingLeft="30dp"
            android:text="Help"
            android:textColor="#575050"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="59dp"
            android:layout_height="37dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="28dp"
            android:background="#FFFFFF"
            app:layout_constraintEnd_toStartOf="@+id/textView6"
            app:layout_constraintHorizontal_bias="0.128"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="377dp"
            android:layout_height="489dp"
            android:layout_marginTop="16dp"
            android:background="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.485"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6">

            <Button
                android:id="@+id/button3"
                android:layout_width="327dp"
                android:layout_height="70dp"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/custom_button"
                android:drawableStart="@drawable/custom_button"
                android:gravity="center|left"
                android:text=" Trip queries"
                android:textAllCaps="false"
                android:textColor="#2C2C2C"
                android:textSize="18sp"
                android:textStyle="bold"
                app:backgroundTint="#F3F3F3"
                app:layout_constraintBottom_toTopOf="@+id/button12"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.325"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/button10"
                android:layout_width="327dp"
                android:layout_height="70dp"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="5dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/custom_button"
                android:drawableStart="@drawable/edit_round"
                android:gravity="center|left"
                android:text="Change pasword"
                android:textAllCaps="false"
                android:textColor="#2C2C2C"
                android:textSize="18sp"
                android:textStyle="bold"
                app:backgroundTint="#F3F3F3"
                app:layout_constraintBottom_toTopOf="@+id/button11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.454" />

            <Button
                android:id="@+id/button11"
                android:layout_width="327dp"
                android:layout_height="70dp"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/custom_button"
                android:drawableStart="@drawable/custom_button"
                android:gravity="center|left"
                android:text="Earn additional money"
                android:textAllCaps="false"
                android:textColor="#2C2C2C"
                android:textSize="18sp"
                android:textStyle="bold"
                app:backgroundTint="#F3F3F3"
                app:layout_constraintBottom_toTopOf="@+id/button3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.325"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/button12"
                android:layout_width="327dp"
                android:layout_height="70dp"
                android:layout_marginStart="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/custom_button"
                android:drawableStart="@drawable/custom_button"
                android:gravity="center|left"
                android:text="Service Quality"
                android:textAllCaps="false"
                android:textColor="#2C2C2C"
                android:textSize="18sp"
                android:textStyle="bold"
                app:backgroundTint="#F3F3F3"
                app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName10"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.3"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/editTextTextPersonName10"
                android:layout_width="321dp"
                android:layout_height="96dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/edit_round"
                android:ems="10"
                android:gravity="center"
                android:hapticFeedbackEnabled="false"
                android:inputType="textPersonName"
                android:text="Write  us"
                android:textColor="#2C2C2C"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.321"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <Button
            android:id="@+id/button13"
            android:layout_width="279dp"
            android:layout_height="44dp"
            android:layout_marginBottom="56dp"
            android:background="@drawable/custom_button"
            android:text="Submit"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            app:backgroundTint="#282F39"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.534"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>